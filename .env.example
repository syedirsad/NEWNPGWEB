# Environment Variables Template
# Copy this file to .env.local and fill in your actual values

# App Configuration
VITE_APP_NAME="NEET Physics Gujarati"
VITE_APP_VERSION="2.0.0"
VITE_APP_DESCRIPTION="Advanced NEET Physics learning platform"

# API Configuration
VITE_API_BASE_URL="https://api.neetphysicsgujarati.com"
VITE_API_TIMEOUT=10000

# Authentication (if using external auth)
VITE_AUTH_DOMAIN="your-auth-domain.auth0.com"
VITE_AUTH_CLIENT_ID="your_auth_client_id"

# Analytics
VITE_GOOGLE_ANALYTICS_ID="GA_MEASUREMENT_ID"
VITE_HOTJAR_ID="your_hotjar_id"

# External Services
VITE_SENTRY_DSN="your_sentry_dsn"
VITE_CRISP_WEBSITE_ID="your_crisp_id"

# Feature Flags
VITE_ENABLE_PWA=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_CHAT_SUPPORT=false

# Development
VITE_DEBUG_MODE=false
VITE_MOCK_API=false

# Social Media
VITE_YOUTUBE_CHANNEL="@npgujarati"
VITE_TELEGRAM_CHANNEL="@neet_physics_gujarati"
VITE_INSTAGRAM_HANDLE="@neet_physics.gujarati"
VITE_WHATSAPP_GROUP="https://chat.whatsapp.com/LGeExJ4a5562e13B04uuQm"

# Contact Information
VITE_CONTACT_EMAIL="syed.irsad@gmail.com"
VITE_CONTACT_PHONE="+919601827762"

# CDN and Assets
VITE_CDN_URL="https://cdn.neetphysicsgujarati.com"
VITE_ASSETS_URL="https://assets.neetphysicsgujarati.com"